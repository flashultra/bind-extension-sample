<?xml version="1.0" encoding="utf-8"?>
<project>
	<meta title="TestAndr" package="com.andr.testandroid" version="1.0.0" company="TAndr" />
	
	<!-- output -->
	<app main="Main" file="TestAndr" path="bin" />
	
	<window background="#000000" fps="60" />
	<window width="800" height="480" unless="mobile" />
	<window orientation="landscape" vsync="false" antialiasing="0" if="cpp" />
	
	<!-- classpath, haxe libs -->
	<source path="src" />
	<haxelib name="openfl" />
	<haxelib name="bind" />

	<template path="templates" if ="android" />
	
	 <android minimum-sdk-version="26" />
	<!-- assets -->
	<assets path="assets/img" rename="img" />
</project>